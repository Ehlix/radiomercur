    "lucide-vue-next": "^0.451.0",
    "radix-vue": "^1.9.7",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.5.11",
    "vue-i18n": "10.0.4",
    "vue-router": "4.4.5"
  },
  "devDependencies": {
    "@cspell/dict-ru_ru": "^2.2.4",
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/geojson": "7946.0.14",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
